{
  "name": "calensync",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@paddle/paddle-js": "^0.5.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.67",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^13.5.0",
    "react-loader-spinner": "^5.4.5",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "react-snap-dominusmi": "file:../../../Downloads/react-snap",
    "react-toastify": "^9.1.3",
    "react-tooltip": "^5.25.0",
    "reactyll": "^0.0.5",
    "rehype-raw": "^7.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:blog": "blogger",
    "build:dev": "PUBLIC_URL=/dev dotenv -e .env.development react-scripts build",
    "build:prod": "dotenv -e .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy:dev": "aws s3 --profile opali sync build s3://calensync/frontend/dev",
    "deploy:prod": "aws s3 --profile opali sync build s3://calensync/frontend/prod",
    "deploy-ci:dev": "aws s3 sync build s3://calensync/frontend/dev",
    "deploy-ci:prod": "aws s3 sync build s3://calensync/frontend/prod && aws cloudfront create-invalidation --distribution-id E295TYJFC3TR5Y --paths \"/*\"",
    "postbuild:dev": "node scripts/generateSitemap.js https://calensync.live && PUBLIC_URL=/dev react-snap && cp sitemap.xml build/",
    "postbuild:prod": "node scripts/generateSitemap.js https://calensync.live && react-snap && cp sitemap.xml build/",
    "sitemap": "node scripts/generateSitemap.js"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--single-process"
    ],
    "sitemapPath": "sitemap.xml",
    "timeout": 60000,
    "concurrency": 1
  },
  "blogger": {
    "template": "src/components/BlogTemplate.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.11",
    "@types/uuid": "^9.0.7",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
