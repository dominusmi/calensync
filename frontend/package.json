{
  "name": "calensync",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "dependencies": {
    "@paddle/paddle-js": "^0.5.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-i18next": "^13.5.0",
    "react-loader-spinner": "^6.1.6",
    "react-markdown": "^9.0.1",
    "react-toastify": "^9.1.3",
    "react-tooltip": "^5.25.0",
    "reactyll": "^1.0.1",
    "rehype-raw": "^7.0.0",
    "typescript": "^4.9.5",
    "vite": "^5.1.5",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "vite-react-ssg build",
    "dev": "NODE_OPTIONS='--max-old-space-size=512000000' vite-react-ssg dev",
    "serve": "vite preview",
    "build-blog": "blogger",
    "build:dev": "BASE_URL=/dev vite-react-ssg build --mode staging",
    "build:prod": "vite-react-ssg build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy:dev": "aws s3 --profile opali sync dist s3://calensync/frontend/dev",
    "deploy:prod": "aws s3 --profile opali sync build s3://calensync/frontend/prod",
    "deploy-ci:dev": "aws s3 sync build s3://calensync/frontend/dev",
    "deploy-ci:prod": "aws s3 sync build s3://calensync/frontend/prod && aws cloudfront create-invalidation --distribution-id E295TYJFC3TR5Y --paths \"/*\"",
    "postbuild:dev": "node scripts/generateSitemap.js https://calensync.live && cp sitemap.xml dist/",
    "postbuild:prod": "node scripts/generateSitemap.js https://calensync.live && cp sitemap.xml build/",
    "sitemap": "node scripts/generateSitemap.js"
  },
  "blogger": {
    "template": "src/components/BlogTemplate.tsx"
  },
  "devDependencies": {
    "@childrentime/devcert": "^1.2.8",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.63",
    "@types/uuid": "^9.0.7",
    "critters": "^0.0.19",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "vite-plugin-cjs-interop": "^2.0.6",
    "vite-react-ssg": "^0.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
